{
  "manifest_version": 3,
  "name": "Chrome Tweet Dialog",
  "version": "1.0",
  "description": "Twitter Webにて「ツイートをする」ボタンを押したときに確認ダイアログを表示してくれます。",
  "icons": {
    "16": "icon-16px.png",
    "48": "icon-48px.png",
    "128": "icon-128px.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}